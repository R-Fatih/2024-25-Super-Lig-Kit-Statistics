@using _2024_25_Süper_Lig_Kit.WebUI.Entities
@model List<JerseyImage>

@{
    ViewData["Title"] = "Index";
}



<table class="table table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>


        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.JerseyImageId</td>
                <td>
                    <img src="@item.ImgPath" alt="@item.ImgPath Logo" width="100" height="100" />
                </td>
               
         

            </tr>
        }
    </tbody>
</table>
<a href="/Jersey/Images/Create/@ViewBag.id" class="btn btn-success">Create New Jersey Image</a>
